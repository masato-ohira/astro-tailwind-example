    "eslint-config-next": "^13.4.19",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.1",
    "fs-extra": "^11.1.1",
    "googleapis": "^126.0.0",
    "lodash": "^4.17.21",
    "ping": "^0.4.4",
    "postcss": "^8.4.28",
